<emitter imageName="" name="Bubble Particle" useAdditive="false" useOriented="false">
    <spawnInterval enabled="false" max="1000.0" min="1000.0"/>
    <spawnCount enabled="true" max="100.0" min="80.0"/>
    <initialLife enabled="true" max="250.0" min="200.0"/>
    <initialSize enabled="true" max="10.0" min="4.0"/>
    <initialDistance enabled="false" max="0.0" min="0.0"/>
    <yOffset enabled="true" max="5.0" min="-5.0"/>
    <xOffset enabled="true" max="5.0" min="-5.0"/>
    <speed enabled="true" max="450.0" min="350.0"/>
    <length enabled="true" max="300.0" min="300.0"/>
    <spread value="360"/>
    <angularOffset value="0.0"/>
    <growthFactor value="0.5"/>
    <gravityFactor value="0.0"/>
    <startAlpha value="255.0"/>
    <endAlpha value="80.0"/>
    <color></color>
</emitter>

